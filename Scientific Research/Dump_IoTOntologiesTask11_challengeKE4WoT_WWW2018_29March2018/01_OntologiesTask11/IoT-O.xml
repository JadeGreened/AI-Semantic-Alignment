    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://iserve.kmi.open.ac.uk/ns/msm#hasInput -->

    <ObjectProperty rdf:about="http://iserve.kmi.open.ac.uk/ns/msm#hasInput"/>
    


    <!-- http://iserve.kmi.open.ac.uk/ns/msm#hasOperation -->

    <ObjectProperty rdf:about="http://iserve.kmi.open.ac.uk/ns/msm#hasOperation"/>
    


    <!-- http://iserve.kmi.open.ac.uk/ns/msm#hasOutput -->

    <ObjectProperty rdf:about="http://iserve.kmi.open.ac.uk/ns/msm#hasOutput"/>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#getsFeedbackFrom -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#getsFeedbackFrom">
        <inverseOf rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#providesFeedbackTo"/>
        <rdfs:domain rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#ControlledSystem"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#System"/>
        <rdfs:comment>Relation between a controlled system and the entities that provide it with a feedback about its action, usually sensors.</rdfs:comment>
        <rdfs:label>gets feedback from</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#grounds -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#grounds">
        <inverseOf rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isGroundedBy"/>
        <rdfs:domain rdf:resource="http://iserve.kmi.open.ac.uk/ns/msm#Operation"/>
        <rdfs:comment>Relationship between a Situation and its concrete implementation. For instance, an Operation can ground an Observation</rdfs:comment>
        <rdfs:label>grounds</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasFeatureOfInterest -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasFeatureOfInterest">
        <inverseOf rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isFeatureOfInterestOf"/>
        <rdfs:domain rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#IoT_Thing"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest"/>
        <rdfs:comment>Relationship between a Thing and a property of the environment. In a cyber-physical system, devices and software agents may have one to many feature of interest from the environment. This feature can be a property, another device, a person...</rdfs:comment>
        <rdfs:label>has feature of interest</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasImpact -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasImpact">
        <inverseOf rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isImpactOf"/>
        <rdfs:domain rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/SAN#Actuation"/>
        <rdfs:range rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#ImpactOnProperty"/>
        <rdfs:comment>Relationship between an operation and its impact on a property</rdfs:comment>
        <rdfs:label>has impact</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasImpactonProperty -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasImpactonProperty">
        <inverseOf rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#propertyIsImpactedBy"/>
        <rdfs:domain rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#ImpactOnProperty"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Property"/>
        <rdfs:comment>Relationship between an impact and the impacted property</rdfs:comment>
        <rdfs:label>on property</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasRelationshipWithProperty -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasRelationshipWithProperty">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#Property"/>
        <rdfs:comment>Generic property that marks the relationship between some entity and some ssn:Property. Dependant on context, it can be sensing, acting or else. Useful to descrie a generic service offered by a complex system that aggregates both sensors and actuators.</rdfs:comment>
        <rdfs:label>has relationship with property</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasService -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasService">
        <inverseOf rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isServiceOf"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:range rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Service"/>
        <rdfs:comment>Relationship between an Entity and a service that is accessible through it.</rdfs:comment>
        <rdfs:label>has service</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isFeatureOfInterestOf -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isFeatureOfInterestOf">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#FeatureOfInterest"/>
        <rdfs:range rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#IoT_Thing"/>
        <rdfs:comment>Indicates that the property is the feature of interest of some entity of an IoT system, that can measure it/act upon it.</rdfs:comment>
        <rdfs:label>is feature of interest for</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isGroundedBy -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isGroundedBy">
        <rdfs:domain>
            <Class>
                <unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN#Actuation"/>
                    <rdf:Description rdf:about="http://purl.oclc.org/NET/ssnx/ssn#Observation"/>
                </unionOf>
            </Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://iserve.kmi.open.ac.uk/ns/msm#Operation"/>
        <rdfs:comment>Inverse of grounds</rdfs:comment>
        <rdfs:label>is grounded by</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isImpactOf -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isImpactOf">
        <rdfs:label>is impact of</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isInputMessageOf -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isInputMessageOf">
        <inverseOf rdf:resource="http://www.wsmo.org/ns/wsmo-lite#hasInputMessage"/>
        <rdfs:label>is input message of</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isServiceOf -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#isServiceOf">
        <rdfs:domain rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Service"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
        <rdfs:comment>Relationship between a service and an entity that accesses it.</rdfs:comment>
        <rdfs:label>is service of</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#propertyIsImpactedBy -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#propertyIsImpactedBy">
        <rdfs:label>property impacted by</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#providesFeedbackTo -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#providesFeedbackTo">
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#System"/>
        <rdfs:range rdf:resource="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#ControlledSystem"/>
        <rdfs:comment>Relationship between an entity that provides informations to a system and this system, that interprets these informations as a feedback of its action.</rdfs:comment>
        <rdfs:label>provides feedback to</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.irit.fr/recherches/MELODI/ontologies/SAN#impactsState -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/SAN#impactsState">
        <rdfs:range rdf:resource="http://purl.org/vocab/lifecycle/schema#State"/>
    </ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParametrizedBy -->

    <ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParametrizedBy"/>
    


    <!-- http://www.wsmo.org/ns/hrests#hasAddress -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasAddress">
        <rdfs:domain rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Operation"/>
        <rdfs:range rdf:resource="http://www.wsmo.org/ns/hrests#URITemplate"/>
        <rdfs:subPropertyOf rdf:resource="http://www.wsmo.org/ns/hrests#hasAddress"/>
    </ObjectProperty>
    


    <!-- http://www.wsmo.org/ns/hrests#hasMethod -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasMethod">
		<rdfs:subPropertyOf rdf:resource="http://www.wsmo.org/ns/hrests#hasMethod"/>
        <rdfs:domain rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Operation"/>
    </ObjectProperty>
    


    <!-- http://www.wsmo.org/ns/wsmo-lite#hasInputMessage -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasInputMessage">
        <rdfs:domain rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Operation"/>
        <rdfs:range rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Message"/>
        <rdfs:subPropertyOf rdf:resource="http://www.wsmo.org/ns/wsmo-lite#hasInputMessage"/>
        <rdfs:comment>Relationship between an Operation that requires some Message as its input</rdfs:comment>
        <rdfs:label>has input message</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.wsmo.org/ns/wsmo-lite#hasOperation -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasOperation">
        <rdfs:domain rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Service"/>
        <rdfs:range rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Operation"/>
        <rdfs:subPropertyOf rdf:resource="http://www.wsmo.org/ns/wsmo-lite#hasOperation"/>
        <rdfs:label>has operation</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.wsmo.org/ns/wsmo-lite#hasOutputMessage -->

    <ObjectProperty rdf:about="http://www.irit.fr/recherches/MELODI/ontologies/IoT-O#hasOutputMessage">
        <rdfs:domain rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Operation"/>
        <rdfs:range rdf:resource="http://www.wsmo.org/ns/wsmo-lite#Message"/>
        <rdfs:subPropertyOf rdf:resource="http://www.wsmo.org/ns/wsmo-lite#hasOutputMessage"/>
        <rdfs:comment>Relashionship between an Operation and the Message that it will output as the result of its processing</rdfs:comment>
        <rdfs:label>has output message</rdfs:label>
    </ObjectProperty>
    



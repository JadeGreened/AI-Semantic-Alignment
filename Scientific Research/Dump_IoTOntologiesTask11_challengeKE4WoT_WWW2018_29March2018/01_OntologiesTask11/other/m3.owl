	<owl:Class rdf:ID="M2MDevice">		
		<rdfs:label xml:lang="en">M2M Device</rdfs:label>
		<rdfs:comment xml:lang="en">M2M device is designed to classify sensors, actuators, RFID tags, controllers and transducers</rdfs:comment>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:modified> 
	</owl:Class>	
	
	<owl:Class rdf:ID="Actuator">		
		<rdfs:label xml:lang="en">Actuator</rdfs:label>
		<rdfs:comment xml:lang="en">Actuator is used to open/close a door, switch on/off the lights, adjust the temperature, etc.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#M2MDevice"/>	
		<owl:equivalentClass rdf:resource="&sosa;Actuator"/>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:modified> 
	</owl:Class>
	
	<owl:Class rdf:ID="Controller">		
		<rdfs:label xml:lang="en">Controller</rdfs:label>
		<rdfs:comment xml:lang="en">Controller</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#M2MDevice"/>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:modified> 	
	</owl:Class>
	
	<owl:Class rdf:ID="Transducer">		
		<rdfs:label xml:lang="en">Transducer</rdfs:label>
		<rdfs:comment xml:lang="en">Transducer</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#M2MDevice"/>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:modified> 	
	</owl:Class>
		
	<owl:Class rdf:ID="RFIDTag">		
		<rdfs:label xml:lang="en">RFID Tag</rdfs:label>
		<rdfs:comment xml:lang="en">RFID tags can be attached to food to design a smart fridge, to CDs for music recommendation, to DVDs for movie recommendation, etc.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#M2MDevice"/>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:modified> 	
	</owl:Class>	

	<owl:Class rdf:ID="Sensor">		
		<rdfs:label xml:lang="en">Physical Sensor</rdfs:label>
		<rdfs:comment xml:lang="en">Sensor used to monitor environment (temperature, humidity, etc.) or other domains such as smart healthcare (glucose level).</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#M2MDevice"/>	
		<owl:equivalentClass rdf:resource="&ssn;Sensor"/>
		<owl:equivalentClass rdf:resource="&ontoSensor;Sensor"/>
		<owl:equivalentClass rdf:resource="&sosa;Sensor"/>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:modified> 
	</owl:Class>	
	
	<owl:Class rdf:ID="Pacemaker">		
		<rdfs:label xml:lang="en">Pacemaker</rdfs:label>
		<rdfs:comment xml:lang="en">Pacemaker used in smart healtcare to help control abnormal heart rhythms.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#M2MDevice"/>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:modified> 	
	</owl:Class>
	
	<owl:Class rdf:ID="Camera">		
		<rdfs:label xml:lang="en">Camera</rdfs:label>
		<rdfs:comment xml:lang="en">Camera is used for video surveillance, to detect presence in a home, etc.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#M2MDevice"/>	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:modified> 
	</owl:Class>	
		
	<owl:Class rdf:ID="MobilePhone">		
		<rdfs:label xml:lang="en">MobilePhone</rdfs:label>
		<rdfs:comment xml:lang="en">Mobile Phone containes various sensors such as accelerometers, etc.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#M2MDevice"/>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:modified> 	
	</owl:Class>
	
	<owl:Class rdf:ID="PDA">		
		<rdfs:label xml:lang="en">PDA (Personal Digital Assistant)</rdfs:label>
		<rdfs:comment xml:lang="en">Before smart phone, there was PDA (Personal Digital Assistant)</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#M2MDevice"/>	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:modified> 
	</owl:Class>
	
	<owl:Class rdf:ID="Computer">		
		<rdfs:label xml:lang="en">Computer, PC</rdfs:label>
		<rdfs:comment xml:lang="en">Computer, PC</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Actuator"/>	
		<rdfs:subClassOf rdf:resource="#HomeM2MDevice"/>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:modified> 
	</owl:Class>
    	
	<owl:Class rdf:ID="M2MApplication">		
		<rdfs:label xml:lang="en">M2M Application</rdfs:label>
		<rdfs:comment xml:lang="en">M2MApplication use to classify ontology-based IoT projects in various domains: healthcare, smart city, IoT, smart home, etc.</rdfs:comment>
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-26</dcterms:modified> 
	</owl:Class>
    
      <owl:Class rdf:ID="WaterBottleSensor">		
		<rdfs:label xml:lang="en">Water Bottle Sensor</rdfs:label>
		<rdfs:comment xml:lang="en">Water bottle sensor provides heart rate measurement in bpm (beat per minute). Useful for kHealth project at Knoesis</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Sensor"/>
		<rdfs:subClassOf rdf:resource="#HealthM2MDevice"/>		
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#produces"/>
             	<owl:someValuesFrom rdf:resource="#WaterQuantity"/>
        	</owl:Restriction>
        </rdfs:subClassOf>	
        <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-23</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-23</dcterms:issued>
	</owl:Class>
    
  <owl:Class rdf:ID="FitbitSensor">		
		<rdfs:label xml:lang="en">Fitbit Sensor</rdfs:label>
		<rdfs:comment xml:lang="en">Fitbit sensor provides heart rate measurement in bpm (beat per minute). Useful for kHealth project at Knoesis</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Sensor"/>
		<rdfs:subClassOf rdf:resource="#HealthM2MDevice"/>		
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#produces"/>
             	<owl:someValuesFrom rdf:resource="#HeartBeat"/>
        	</owl:Restriction>
        </rdfs:subClassOf>	
        <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-23</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-23</dcterms:issued>
	</owl:Class>

	<owl:Class rdf:ID="HeartBeat">		
		<rdfs:label xml:lang="en">Heart beat, heart rate</rdfs:label>
		<rdfs:comment xml:lang="en">Heart rate measurement, unit bpm. Useful for kHealth projects (asthma, bariatric surgery, etc.) at Knoesis.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#MeasurementType"/>
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#hasUnit"/>
             	<owl:someValuesFrom rdf:resource="#BeatPerMinute"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
        <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-23</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-01-01</dcterms:issued>
	</owl:Class>

	<owl:Class rdf:ID="BeatPerMinute">		
		<rdfs:label xml:lang="en">Beat Per Minute (bpm)</rdfs:label>
		<rdfs:comment xml:lang="en">Beat Per Minute (bpm) is the unit to measure heart rate. Useful for kHealth projects (asthma, bariatric surgery, etc.) at Knoesis.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Unit"/>	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-23</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-01-01</dcterms:issued>
	</owl:Class>
	
		
	<owl:Class rdf:ID="WeightSensor">		
		<rdfs:label xml:lang="en">Weight Sensor</rdfs:label>
		<rdfs:comment xml:lang="en">Weight Sensor (e.g. weighting scale form Nokia) to measure your weight in lb or kg. Useful for kHealth projects (asthma, bariatric surgery, etc.) at Knoesis.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#Sensor"/>
		<rdfs:subClassOf rdf:resource="#HealthM2MDevice"/>
		<txn:hasImage rdf:resource="http://sensormeasurement.appspot.com/images/sensor/weight.png"/>
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#produces"/>
             	<owl:someValuesFrom rdf:resource="#Weight"/>
        	</owl:Restriction>
        </rdfs:subClassOf>	
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-23</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-01-01</dcterms:issued> 
	</owl:Class>
	
	<owl:Class rdf:ID="Weight">		
		<rdfs:label xml:lang="en">Weight</rdfs:label>
		<rdfs:comment xml:lang="en"></rdfs:comment>
		<rdfs:subClassOf rdf:resource="#MeasurementType"/>
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#hasUnit"/>
             	<owl:someValuesFrom rdf:resource="#Kilo"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#hasUnit"/>
             	<owl:someValuesFrom rdf:resource="#Pound"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
        <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-23</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-01-01</dcterms:issued>
	</owl:Class>
	
		<owl:Class rdf:ID="WaterQuantity">		
		<rdfs:label xml:lang="en">Water Quantity</rdfs:label>
		<rdfs:comment xml:lang="en">WaterQuantity, unit ounce or liter. Useful for kHealth projects (asthma, bariatric surgery, etc.) at Knoesis.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="#MeasurementType"/>
		<rdfs:subClassOf>
			<owl:Restriction>
            	<owl:onProperty rdf:resource="#hasUnit"/>
             	<owl:someValuesFrom rdf:resource="&xsd;double"/>
        	</owl:Restriction>
        </rdfs:subClassOf>
        <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-03-23</dcterms:modified> 
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-01-01</dcterms:issued>
	</owl:Class>
  
